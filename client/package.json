{
  "name": "client",
  "version": "0.3.1",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --prod --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:aot": "npx ng serve --port 4220 -o --aot --optimization --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "start": "npx ng serve",
    "build": "npx ng build",
    "build:prod": "npx ng build --prod",
    "build:prod:silent": "npx ng build --prod --no-progress",
    "test": "npx ng test",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.9",
    "@angular/cdk": "6.4.7",
    "@angular/common": "6.1.9",
    "@angular/compiler": "6.1.9",
    "@angular/core": "6.1.9",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/forms": "6.1.9",
    "@angular/http": "6.1.9",
    "@angular/material": "6.2.1",
    "@angular/platform-browser": "6.1.9",
    "@angular/platform-browser-dynamic": "6.1.9",
    "@angular/router": "6.1.9",
    "core-js": "2.5.7",
    "deep-copy": "1.4.2",
    "deep-object-diff": "1.1.0",
    "loglevel": "^1.6.1",
    "rxjs": "6.3.3",
    "traverse": "0.6.6",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.3",
    "@angular/cli": "~6.2.3",
    "@angular/compiler-cli": "6.1.9",
    "@angular/language-service": "6.1.9",
    "@types/loglevel": "^1.5.3",
    "@types/node": "~8.9.4",
    "@types/traverse": "^0.6.32",
    "codelyzer": "~4.2.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.1"
  }
}
