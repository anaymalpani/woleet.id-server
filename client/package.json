{
  "name": "client",
  "version": "0.3.1",
  "description": "Woleet.ID Server web client",
  "scripts": {
    "ng": "npx ng",
    "dev": "npx ng serve --port 4220 -o --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:prod": "npx ng serve --port 4220 -o --prod --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "dev:aot": "npx ng serve --port 4220 -o --aot --optimization --ssl --ssl-key $WOLEET_ID_SERVER_HTTP_TLS_KEY --ssl-cert $WOLEET_ID_SERVER_HTTP_TLS_CERTIFICATE",
    "start": "npx ng serve",
    "build": "npx ng build",
    "build:prod": "npx ng build --prod",
    "build:prod:silent": "npx ng build --prod --no-progress",
    "lint": "npx ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/material": "^6.4.7",
    "@angular/flex-layout": "^6.0.0-beta.18",
    "@angular/router": "^6.1.10",
    "core-js": "^2.5.7",
    "deep-copy": "^1.4.2",
    "deep-object-diff": "^1.1.0",
    "libphonenumber-js": "^1.7.13",
    "loglevel": "^1.6.1",
    "rxjs": "^6.4.0",
    "timestring": "^5.0.1",
    "traverse": "^0.6.6",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.6",
    "@angular/cli": "^6.2.9",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/language-service": "^6.1.10",
    "@types/loglevel": "^1.5.4",
    "@types/node": "^8.10.45",
    "@types/traverse": "^0.6.32",
    "codelyzer": "^4.5.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.14.0",
    "typescript": "^2.9.2"
  }
}
